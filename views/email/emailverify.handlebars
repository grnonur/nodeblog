<!--==========================
    INSIDE HERO SECTION Section
============================-->
<section class="page-image page-image-contact md-padding">
	<h1 class="text-white text-center">Account Verification</h1>
</section>

<!--==========================
    Contact Section
============================-->
<section id="contact" class="md-padding">
	<div class="container">

		<div class="row text-center">
			<div class="col-md-4 offset-md-4">
				<div class="section-header">
					<h2 class="title">
						{{#unless sessionFlash.message}}
							404 Not Found
						{{/unless}}
						{{#if sessionFlash.message}}
							Service
						{{/if}}
					</h2>
				</div>
			</div>
		</div>

		<div class="row">
			<div class="col-lg-12">
				
					<div class="row">
						<div class="offset-2 col-md-8">
							{{#if sessionFlash.message}}
								<div class="{{sessionFlash.type}}">{{sessionFlash.message}}</div>
							{{/if}}
						</div>
					</div>
				
			</div>
		</div>

	</div>
</section>
